<h1>Signup</h1>

<%= form_for(@user) do |u| %>
	<%= u.label :name %>
	<%= u.text_field :name %>

	<%= u.label :height %>
	<%= u.text_field :height %>

	<%= u.label :happiness %>
	<%= u.text_field :happiness %>

	<%= u.label :nausea %>
	<%= u.text_field :nausea %>

	<%= u.label :tickets %>
	<%= u.text_field :tickets %>

	<%= u.label :password %>
	<%= u.text_field :password %>

	<%= u.submit %>
<% end %>